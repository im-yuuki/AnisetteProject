{
	"$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
	"name": "anisette",
	"version": "1.0.0",
	"dependencies": [
		{
			"name": "sdl2",
			"version>=": "2.32.2"
		},
		{
			"name": "sdl2-image",
			"features": [
				"libjpeg-turbo"
			],
			"version>=": "2.8.8#1"
		},
		{
			"name": "sdl2-mixer",
			"features": [
				"mpg123",
				"wavpack"
			],
			"version>=": "2.8.1"
		},
		{
			"name": "sdl2-ttf",
			"version>=": "2.24.0"
		},
		{
			"name": "sdl2-gfx",
			"version>=": "1.0.4#11"
		},
		{
			"name": "spdlog",
			"version>=": "1.15.1"
		},
		{
			"name": "rapidjson",
			"version>=": "2025-02-26"
		},
		{
			"name": "discord-game-sdk",
			"version>=": "3.2.1"
		}
	]
}
